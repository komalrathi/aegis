let numerator : (int, Low) = 10 in {
  let denominator : (int, Low) = 0 in {
    try {
        if (denominator == 0) then {
            raise!(DivisionByZero denominator)
        } 
        else {
            numerator / denominator
        }
    } 
    catch (DivisionByZero ex k) {
        print(numerator);
        continue (k, 5)
    }
    finally {
        numerator := 49;
        print(numerator)
    }
  }
}
